@model List<CheeseMVC.Models.Menu>
@{
    ViewData["Title"] = "Index";
}

<h1>Menus</h1>

<ul>
    @foreach(var menu in Model)
    {
        <li>
            <a asp-controller="Menu" asp-action="ViewMenu" asp-route-id="@menu.ID">@menu.Name</a>
        </li>
    }
</ul>
<a asp-controller="Menu" asp-action="Add">Add Menu</a>

